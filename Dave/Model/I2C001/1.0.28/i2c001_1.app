  <provided xsi:type="ResourceModel:IntegerParameter" name="Selects desired Bit Rate(kHz)" evalFunction="&#xD;&#xA;&#x9;&#x9;&#x9;function ForwardMapping()&#xD;&#xA;&#x9;&#x9;&#x9;{&#x9;&#x9;&#x9;&#x9;&#x9;&#xD;&#xA;                var XMC1xxx=SCM.getSoftwareId().substring(0,1).compareTo(&quot;1&quot;);  &#xD;&#xA;                if(XMC1xxx==0)&#xD;&#xA;&#x9;            {&#xD;&#xA;                   var Res1 = SCM.getResource(&quot;clockapp/clk002_irMCLK&quot;);&#xD;&#xA;                   var clock = SCM.getDoubleValue(Res1);&#xD;&#xA;&#x9;            }&#xD;&#xA;&#x9;            else&#xD;&#xA;&#x9;            {&#xD;&#xA;&#x9;               var Res1 = SCM.getResource(&quot;clockapp/clk001_iroActualFreqPB&quot;);&#xD;&#xA;                   var clock = SCM.getIntValue(Res1);&#xD;&#xA;     &#x9;        }&#xD;&#xA;     &#xD;&#xA;                var bpdiv = 0;                  &#xD;&#xA;                var BaudEntered = SCM.getIntValue(currentResource); &#xD;&#xA;                if(BaudEntered &lt;= 100)&#xD;&#xA;                {&#xD;&#xA;                  var ratio1 = (((clock * 1000000)/(10*1024*BaudEntered * 1000))*500);&#xD;&#xA;                }&#xD;&#xA;                else&#xD;&#xA;                {&#xD;&#xA;                  var ratio1 = (((clock * 1000000)/(25*1024*BaudEntered*1000))*500); &#xD;&#xA;                }   &#xD;&#xA;                if(ratio1 &lt;= 1) &#xD;&#xA;                {&#xD;&#xA;                  bpdiv = 0;&#xD;&#xA;                }  &#xD;&#xA;                else &#xD;&#xA;                {&#xD;&#xA;                  bpdiv =  (Math.round(ratio1)-1);&#xD;&#xA;                  if( bpdiv > 1023)&#xD;&#xA;                  {&#xD;&#xA;                    bpdiv = 1023;&#xD;&#xA;                  }&#xD;&#xA;                }  &#xD;&#xA;                var Res = SCM.getResource(&quot;channel/brg/pdiv&quot;);                     &#xD;&#xA;                SCM.setIntValue(Res,bpdiv);&#xD;&#xA;                if(BaudEntered &lt;= 100)&#xD;&#xA;    